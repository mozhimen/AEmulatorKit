<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AGameKit_Emulator" parent="Theme.Material3.Dark.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryVariant</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorSecondary</item>
        <item name="colorSecondaryVariant">@color/colorSecondaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="appBarLayoutStyle">@style/LemuroidMaterialStyle.AppBarLayoutStyle</item>
        <item name="materialCardViewStyle">?attr/materialCardViewElevatedStyle</item>
        <item name="alertDialogTheme">@style/LemuroidMaterialStyle.AlertDialogTheme</item>
        <item name="android:statusBarColor">#212121</item>
        <item name="android:navigationBarColor">#242424</item>
        <item name="elevationOverlayColor">?attr/colorOnSurface</item>
    </style>
</resources>